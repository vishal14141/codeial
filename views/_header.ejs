<header>
  <h2>This is header</h2>

    <ul>
        <% if(locals.user) { %>

            <li>
                <a href="/users/profile"><%= user.name %> </a>
            </li>
            <li>
                <a href="/users/sign-out">Sign out</a>
            </li>

        <% } else{ %>

            <li>
                <a href="/users/login">Sign in</a>
            </li>

            <li>
                <a href="/users/register">Sign up</a>
            </li>

            <% }%>
    </ul>
</header>